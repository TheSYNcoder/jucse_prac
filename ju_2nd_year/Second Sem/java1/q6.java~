import java.util.*;


class Test{


    void show(int data){
        System.out.println(data);
    }
    
    void show( double data ){
        System.out.println(data);
    }
    
    
    public  void main(String args[]){
     
        short a = 4;
        double  b = 9.5;
        show(a);
        show(b);       
    }

}
